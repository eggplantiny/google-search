{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["es2022", "dom"],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
